@{
    ViewData["Title"] = "Track Claims";
}
<h1>@ViewData["Title"]</h1>
<div class="tracking-form-container">
    <div class="card-header">
        <h2>Track Your Claim</h2>
    </div>
    <div class="card-body">
        <form id="trackingForm">
            <div class="form-group">
                <label for="trackingId">Tracking ID:</label>
                <input type="text" id="trackingId" name="trackingId" placeholder="Enter tracking ID" required>
            </div>
            <div class="form-group">
                <button type="submit">Track Claim</button>
            </div>
        </form>
    </div>
   
</div>
